<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bxtyfufff.mytest" >
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-sdk android:targetSdkVersion="19"
        android:minSdkVersion="11"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FragmentActivity"
            android:label="@string/title_activity_fragment" >
        </activity>
        <activity
            android:name="com.com.News.helper.NewsContentActivity"
            android:label="@string/title_activity_news_content" >
        </activity>
        <activity
            android:name=".GridViewActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_grid_view"
            android:theme="@style/FullscreenTheme" >
        </activity>
        <activity
            android:name=".ReceiverActivity"
            android:label="@string/title_activity_receiver" >
        </activity>
        <activity
            android:name=".WebeiwActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_webeiw"
            android:theme="@style/FullscreenTheme" >
        </activity>
        <activity android:name=".ExpandableListVIewActivity"
                  android:label="ExpandableListViewActivity"
                  />
    </application>
</manifest>
